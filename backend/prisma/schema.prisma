generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Deal {
  id            String    @id @default(uuid())
  amount        Float
  salesperson   String
  date          DateTime
  commission    Float
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
}